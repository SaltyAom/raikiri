{
  "name": "raikiri",
  "version": "0.0.0-beta.6",
  "type": "module",
  "description": "A fast router optimized for static path searching",
  "author": {
    "name": "saltyAom",
    "url": "https://github.com/SaltyAom",
    "email": "saltyaom@gmail.com"
  },
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/index.js"
  },
  "types": "./src/index.ts",
  "keywords": [
    "elysia",
    "bearer",
    "authentication"
  ],
  "homepage": "https://github.com/saltyaom/raikiri",
  "repository": {
    "type": "git",
    "url": "https://github.com/saltyaom/raikiri"
  },
  "bugs": "https://github.com/saltyaom/raikiri/issues",
  "license": "MIT",
  "scripts": {
    "dev": "bun run --hot example/index.ts",
    "test": "bun wiptest",
    "build": "rimraf dist && tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "benchmark": "bun benchmarks/index.ts",
    "benchmark:raikiri": "bun benchmarks/libs/raikiri.ts",
    "release": "npm run build && npm run test && npm publish --access public"
  },
  "devDependencies": {
    "@medley/router": "^0.2.1",
    "@types/node": "^18.11.7",
    "@types/trouter": "^3.1.1",
    "bun-types": "^0.5.0",
    "express": "^4.18.2",
    "find-my-way": "^7.4.0",
    "hono": "^2.7.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "radix3": "^1.0.0",
    "trouter": "^3.2.0"
  }
}